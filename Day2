print(a) 
#ans 3 a+=b . a=a+b

#subtract and assign
a=1
b=2
a-=b
print(a) 

#multile and assign
a=1
b=2
a*=b
print(a) 
#div and assign
a=1
b=2
a/=b
print(a) 
#modulus and assign
a=1
b=2
a%=b
print(a) 
#floor and assign
a=1
b=2
a//=b
print(a) 

